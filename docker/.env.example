# Environment variables for Voice Ledger Docker deployment
# Copy this file to .env and fill in your values

# OpenAI API Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Voice Ledger API Configuration
VOICE_LEDGER_API_KEY=your-voice-ledger-api-key-here

# Blockchain Configuration
BLOCKCHAIN_RPC_URL=http://blockchain:8545

# Service URLs (internal to Docker network)
VOICE_API_URL=http://voice-api:8000
DPP_RESOLVER_URL=http://dpp-resolver:8001
